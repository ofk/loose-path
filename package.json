{
  "name": "loose-path",
  "version": "1.2.1",
  "description": "",
  "license": "MIT",
  "author": "ofk <ofkjpn+npm@gmail.com> (https://github.com/ofk/)",
  "homepage": "https://github.com/ofk/loose-path#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:ofk/loose-path.git"
  },
  "bugs": {
    "url": "https://github.com/ofk/loose-path/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "lint": "run-p lint:* prettier:lint",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:package-json": "prettier-package-json -l",
    "lint:tsc": "tsc",
    "lint-fix": "npm-run-all -p lint-fix:* -s prettier:lint-fix",
    "lint-fix:eslint": "npm run lint:eslint -- --fix",
    "lint-fix:package-json": "prettier-package-json --write",
    "prepare": "husky install",
    "prettier:lint": "prettier . --check",
    "prettier:lint-fix": "npm run prettier:lint -- --write",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@ofk/eslint-config-recommend": "^5.3.1",
    "@types/jest": "^29.5.1",
    "eslint": "^8.41.0",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-package-json": "^2.8.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "keywords": [],
  "lint-staged": {
    "package.json": "prettier-package-json --write",
    "*.{md,json,html}": "prettier --write",
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --ext .js,.jsx,.ts,.tsx",
      "prettier --write"
    ]
  }
}
